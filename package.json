    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-test-renderer": "^18.0.0",
    "@types/recoil": "^0.0.9",
    "@types/styled-components": "^5.1.26",
    "cypress": "^12.12.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
